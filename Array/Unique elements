#include <stdio.h>

int main() {
    int arr[100], n, i, j, count;

    printf("Enter number of elements in the array: ");
    scanf("%d", &n);

    printf("Enter the elements of the array:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Unique elements of array are: ");
    for(i = 0; i < n; i++) {
        count = 0; // set to zero at every iteration 
        for(j = 0; j < n; j++) {
            if(arr[i] == arr[j]) {
                count++; // counts
            }
        }

        if(count == 1) { // appears just once
            printf("%d ", arr[i]);
        }
    }

    printf("\n");
    return 0;
}
