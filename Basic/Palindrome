#include <stdio.h>
int main(){
    int num,original,reversed=0,rem;
    
    printf("Enter a number:");
    scanf("%d",&num);
    
    original = num;
    
    while(num!=0){
        rem = num%10;
        reversed = reversed*10+rem;
        num= num/10;
    }
  printf("Reversed number:%d\n",reversed);
  
  if(original==reversed){
      printf("%d is palindrome.",original);
  }
      else{
           printf("%d is not palindrome.",original);
      }
  
  
  return 0;
}
