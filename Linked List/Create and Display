#include <stdio.h>
#include <stdlib.h>

struct node{
    int data;
    struct node *next;
};

int main(){
    int i,n,element;
    struct node *temp,*perm,*head;
    
    printf("Enter number of nodes:");
    scanf("%d",&n);
    printf("Enter value for the head node:");
    scanf("%d",&element);
    
    temp=(struct node*)malloc(sizeof (struct node));
    temp ->data = element;
    temp ->next = NULL;
    
    perm = temp;
    head = temp;
    
    for(i=1;i<n;i++){
    printf("Enter value for next node:");
    scanf("%d",&element);
    
    temp=(struct node*)malloc(sizeof (struct node));
    temp ->data = element;
    temp ->next = NULL;
    
    perm ->next = temp;
    perm = perm->next;
        
    }
    perm = head;
    while(perm!=NULL){
        printf(" %d ",perm->data);
        printf(" %d ",perm->next);
        perm=perm->next;
    }
    
    
return 0;
}
