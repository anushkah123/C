#include<stdio.h>
int main(){
    int arr[]={3,5,4,7,8,1};
    int i,j,temp;
    
    for(i=0;i<5;i++){
        for(j=i+1;j<6;j++){
            if(arr[i]>arr[j]){
                temp = arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
            }
        }
    }
    
    printf("Array in sorted order is:\n");
    for(i=0;i<6;i++){
        printf(" %d ",arr[i]);
    }
return 0;
}
