#include <stdio.h>
int main(){
    int arr[50],n,i,key;
    
    printf("Enter number of elements in the array:");
    scanf("%d",&n);
    
    printf("Enter the elements of the array:");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    
    printf("Element you want to search:");
    scanf("%d",&key);

  for(i=0;i<n;i++){
    if (arr[i]==key){
        printf("Element %d found at position : %d",arr[i],i+1);
        break;
    }
}
    
   if(i==n){
        printf("Element not found.");
    }

    
return 0;
}
